    "@grammyjs/parse-mode": "1.11.1",
    "@nuxt/ui": "3.3.0",
    "@pinia/nuxt": "0.11.2",
    "@roziscoding/grammy-autoquote": "2.0.9",
    "@telegram-apps/init-data-node": "2.0.10",
    "@vueuse/core": "13.6.0",
    "awilix": "12.0.5",
    "dayjs": "1.11.13",
    "dotenv": "17.2.1",
    "es-toolkit": "1.39.8",
    "fluent-vue": "3.7.2",
    "grammy": "1.37.0",
    "h3": "1.15.3",
