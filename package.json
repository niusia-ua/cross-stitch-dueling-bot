    "nuxt": "4.0.1",
    "ofetch": "1.4.1",
    "pinia": "3.0.3",
    "slonik": "48.3.0",
    "vue": "3.5.17",
    "vue-router": "4.5.1",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@iconify-json/lucide": "1.2.58",
    "@nuxt/eslint": "1.7.1",
    "@nuxt/test-utils": "3.19.2",
    "@types/jsonwebtoken": "9.0.10",
    "@types/telegram-web-app": "7.10.1",
    "@vue/eslint-config-prettier": "10.2.0",
    "eslint": "9.32.0",
    "node-pg-migrate": "9.0.0-alpha.0",
    "prettier": "3.6.2",
    "prettier-plugin-sql": "0.19.2",
