    "@pinia/nuxt": "^0.11.2",
    "awilix": "^12.0.5",
    "dotenv": "^17.2.1",
    "fluent-vue": "^3.7.2",
    "grammy": "^1.37.0",
    "h3": "^1.15.3",
    "nuxt": "^4.0.1",
    "pinia": "^3.0.3",
    "slonik": "^47.3.2",
    "slonik-interceptor-field-name-transformation": "^47.3.2",
